{
  "name": "jeremy-personal-site",
  "private": true,
  "description": "Personal website using on gatsby and react",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=development gatsby build",
    "develop": "gatsby develop",
    "deploy": "gatsby build && gh-pages -d public --branch master",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.5.1",
    "axios": "^0.19.2",
    "gatsby-plugin-google-analytics": "^2.1.34",
    "gatsby": "^2.19.12",
    "gatsby-plugin-gtag": "^1.0.12",
    "gatsby-plugin-material-ui": "latest",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-source-graphql": "^2.1.33",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.0",
    "gh-pages": "^2.2.0",
    "prettier": "^1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeremyaRD/personal-site"
  },
  "bugs": {
    "url": "https://github.com/jeremyaRD/personal-site/issues"
  }
}
